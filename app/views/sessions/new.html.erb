<h1>Log in</h1>
<%= render 'layouts/loglinks' %>

<%= form_with model: @user, url: sessions_path, local: true do |f| %>
<div class="card col-md-6">
  <p>
  <%= f.label :email %>
  <%= f.text_area :email %>
  </p>  
  <p>
  <%= f.label :password %>
  <%= f.password_field :password %>
  </p>
  <%= f.submit "Entrar" %>
</div>
<% end %>
